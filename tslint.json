{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration"
    ],
    "no-string-literal": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-line-length": [
      false
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ]
  },
  "rulesDirectory": []
}